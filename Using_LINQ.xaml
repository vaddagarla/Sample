            </Assign.To>
            <Assign.Value>
              <InArgument x:TypeArguments="x:Object" xml:space="preserve">[From a In DataTable1.AsEnumerable()
             Group Join b In DataTable2.AsEnumerable() 
             On a("External BOL No").ToString() Equals b("Bol No#").ToString() Into MatchedRows = Group
             From mRow In MatchedRows.DefaultIfEmpty()
             Let quantityA = If(String.IsNullOrEmpty(a("Quantity").ToString()), 0, Convert.ToDecimal(a("Quantity")))
             Let quantityB = If(mRow Is Nothing, 0, Convert.ToDecimal(mRow("Quantity")))
             Let status = If(String.IsNullOrEmpty(a("YourColumn").ToString()), "No", "Yes")
             Let remarks = If(String.IsNullOrEmpty(a("YourColumn").ToString()), "Empty", 
                              If(quantityA = quantityB, "Matched", "Not Matched"))
             Select New With {
                 .BOLNumber = a("BOL number #").ToString(),
                 .QuantityA = quantityA,
                 .QuantityB = quantityB,
                 .Status = status,
                 .Remarks = remarks
             }]</InArgument>
            </Assign.Value>
